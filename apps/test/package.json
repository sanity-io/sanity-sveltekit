{
  "name": "apps-test",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "typegen": "dotenv -e .env -- npm run typegen:extract && npm run typegen:generate",
    "typegen:extract": "cd src/lib/sanity && sanity schema extract",
    "typegen:generate": "sanity typegen generate"
  },
  "dependencies": {
    "@sanity/client": "^7.15.0",
    "@sanity/image-url": "^2.0.3",
    "groq": "^5.11.0",
    "sanity": "^5.11.0"
  },
  "devDependencies": {
    "@sanity/sveltekit": "workspace:*",
    "@sveltejs/adapter-vercel": "^6.3.0",
    "@sveltejs/kit": "^2.50.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "svelte": "^5.47.1",
    "svelte-check": "^4.3.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
