{
  "name": "@sanity/sveltekit",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/sanity-sveltekit.git",
    "directory": "packages/sanity-sveltekit"
  },
  "scripts": {
    "dev": "svelte-package --watch",
    "build": "npm run prepare && vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.16.1",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.27.0",
    "@sanity/types": "^3.90.0",
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.21.1",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.9.0",
    "globals": "^16.2.0",
    "publint": "^0.3.12",
    "svelte": "^5.33.6",
    "svelte-check": "^4.2.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@sanity/client": "^7.3.0",
    "@sanity/comlink": "^3.0.5",
    "@sanity/core-loader": "^1.8.10",
    "@sanity/presentation-comlink": "^1.0.20",
    "@sanity/preview-url-secret": "^2.1.11",
    "@sanity/visual-editing": "^2.15.0",
    "fast-deep-equal": "^3.1.3",
    "sanity": "^3.90.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
