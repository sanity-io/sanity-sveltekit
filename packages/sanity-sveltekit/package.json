{
  "name": "@sanity/sveltekit",
  "version": "1.0.4",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/sanity-sveltekit.git",
    "directory": "packages/sanity-sveltekit"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "build": "npm run prepare && vite build && npm run prepack",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "svelte-package --watch",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "test": "npm run test:unit -- --run",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@sanity/client": "^7.12.0",
    "@sanity/comlink": "^4.0.0",
    "@sanity/core-loader": "^2.0.0",
    "@sanity/presentation-comlink": "^2.0.0",
    "@sanity/preview-url-secret": "^2.1.15",
    "@sanity/visual-editing": "^4.0.0",
    "fast-deep-equal": "^3.1.3",
    "groq": "^4.11.0",
    "sanity": "^4.11.0"
  },
  "devDependencies": {
    "@sanity/types": "^4.12.0",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.47.3",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "publint": "^0.3.15",
    "svelte": "^5.42.1",
    "svelte-check": "^4.3.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.16.1",
    "svelte": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
