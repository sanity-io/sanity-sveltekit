{
  "name": "@sanity/sveltekit",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/sanity-sveltekit.git",
    "directory": "packages/sanity-sveltekit"
  },
  "scripts": {
    "dev": "svelte-package --watch",
    "build": "npm run prepare && vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.16.1",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sanity/types": "^3.99.0",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/kit": "^2.43.5",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "publint": "^0.3.13",
    "svelte": "^5.39.7",
    "svelte-check": "^4.3.2",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@sanity/client": "^7.11.2",
    "@sanity/comlink": "^3.0.9",
    "@sanity/core-loader": "^1.8.18",
    "@sanity/presentation-comlink": "^1.0.29",
    "@sanity/preview-url-secret": "^2.1.15",
    "@sanity/visual-editing": "^2.15.4",
    "fast-deep-equal": "^3.1.3",
    "sanity": "^3.99.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
